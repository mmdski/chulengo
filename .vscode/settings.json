{
  "editor.tabSize": 2,
  "files.associations": {
    "*.h": "c"
  },
  "cmake.configureArgs": ["-DTESTS=1", "-DEXAMPLES=1"],
  "cSpell.words": [
    "BEAVCREK",
    "chlib",
    "chulengo",
    "Domanski",
    "dtype",
    "libchlib",
    "linestyle",
    "markerfacecolor",
    "ndim",
    "subsect",
    "xlabel",
    "ylabel"
  ],
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "examples/ras/*"
  ]
}
