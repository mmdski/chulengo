  "editor.tabSize": 2,
  "files.associations": {
    "*.h": "c"
  },
  "cmake.configureArgs": ["-DTESTS=1", "-DEXAMPLES=1"],
  "cSpell.words": ["chlib", "chulengo", "Domanski", "dtype", "libchlib"],
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "examples/ras/*"
  ]
}
